{
"manifest_version": 2,
"name": "Useless color saver",
"version": "1.5",
"content_security_policy": "default-src *; style-src 'self' 'unsafe-inline'; script-src 'self' 'unsafe-inline' 'unsafe-eval'",
"description": "Is useless",
"content_scripts": [
  {
    "matches": ["<all_urls>"],
    "js": ["script.js"]
  }
],

"browser_action": {
  "default_title": "A useless color saver",
  "default_popup": "index.html"
},
"icons": {
  "48": "icon-48.png",
  "96": "icon-96.png"
}
}
